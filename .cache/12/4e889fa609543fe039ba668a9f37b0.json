\n        flex: 1 1 40;\r\n        margin-top: 8rem;\r\n        h1 {\r\n            font-size: 36px;\r\n            margin-bottom: 2rem;\r\n        }\r\n        p {\r\n            margin-bottom: 3rem;\r\n        }\r\n        .button {\r\n            padding: 0.5rem;\r\n            color: $b-main;\r\n            background-color: $c-main;\r\n        }\r\n    }\r\n    .hero__right {\r\n        flex: 1 1 40;\r\n        margin: 2rem 0 0 1.5rem;\r\n        img {\r\n            width: 500px;\r\n        }\r\n        .go-down {\r\n            position: absolute;\r\n            left: 90%;\r\n        }\r\n    }\r\n}\r\n\r\n.goods {\r\n    min-height: 600px;\r\n    height: 100%;\r\n    background-color: $w-main;\r\n    .title {\r\n        margin: 2rem;\r\n        h1 {\r\n            text-align: center;\r\n            font-size: 32px;\r\n        }\r\n    }\r\n    .goods-center {\r\n        display: grid;\r\n        grid-template-columns: repeat(3, minmax(auto, 1fr));\r\n        gap: 2rem 2rem;\r\n        justify-content: center;\r\n        text-align: center;\r\n        padding-bottom: 2rem;\r\n        .goods-card {\r\n            display: flex;\r\n            flex-direction: column;\r\n            flex-wrap: wrap;\r\n            .goods__img {\r\n                img {\r\n                    width: 90%;\r\n                }\r\n            }\r\n            .goods__main {\r\n                margin: auto;\r\n                h4 {\r\n                    margin: 0.3rem;\r\n                }\r\n                button {\r\n                    border: none;\r\n                    background-color: $c-main;\r\n                    color: $b-main;\r\n                    padding: 0.3rem;\r\n                    font-size: 20px;\r\n                    cursor: pointer;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.contact {\r\n    background-color: $c-main;\r\n    padding: 1.5rem 0;\r\n    h2 {\r\n        text-align: center;\r\n        color: $b-main;\r\n        font-size: 28px;\r\n    }\r\n    .contact-form {\r\n        input[type=\"text\"],\r\n        select,\r\n        textarea {\r\n            width: 100%;\r\n            padding: 12px;\r\n            border: 1px solid $w-main;\r\n            border-radius: 4px;\r\n            box-sizing: border-box;\r\n            margin-top: 6px;\r\n            margin-bottom: 8px;\r\n            resize: vertical;\r\n        }\r\n\r\n        input[type=\"submit\"] {\r\n            background-color: $w-main;\r\n            color: $c-main;\r\n            padding: 6px 10px;\r\n            border: none;\r\n            border-radius: 4px;\r\n            cursor: pointer;\r\n        }\r\n\r\n        input[type=\"submit\"]:hover {\r\n            background-color: lightblue;\r\n        }\r\n    }\r\n}\r\n\r\nfooter {\r\n    background-color: $b-main;\r\n    padding: 1rem;\r\n    .socialmedia-and-copyright {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        img {\r\n            width: 3%;\r\n        }\r\n        .social {\r\n            flex: 1;\r\n        }\r\n        .copyright {\r\n            margin-right: 2rem;\r\n        }\r\n    }\r\n    .go-up {\r\n        position: absolute;\r\n        right: 5%;\r\n    }\r\n}\r\n\r\n.flex__center {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n.header-logo {\r\n    height: 15vh;\r\n}\r\n\r\n.goods-container{\r\n    margin-top: 1rem;\r\n    background-color: $w-main;\r\n\r\n    .product-header{\r\n        display: flex;\r\n        justify-content: flex-start;\r\n        margin-right: 3rem;\r\n        border-bottom: 4px solid $c-main;\r\n        .product-number{\r\n            width: 10%;\r\n        }\r\n        .product-title{\r\n            width: 25%;\r\n        }\r\n        .price{\r\n            width: 25%;\r\n            display: flex;\r\n            align-items: center;\r\n        }\r\n        .quantity{\r\n            width: 30%;\r\n            display: flex;\r\n            text-align: center;\r\n        }\r\n        .product{\r\n            width: 45%;\r\n            display: flex;\r\n            justify-content: space-around;\r\n            align-items: center;\r\n            padding: 10px 0;\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"816dc17140c3fc00562adf97f3d9a515","cacheData":{"env":{}}}